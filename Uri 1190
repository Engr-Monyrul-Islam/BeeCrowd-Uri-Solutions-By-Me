/* Bismillah */
#include <iostream>
#include <iomanip>
#include <string>
using namespace std;
int main()
{
        double M[12][12];
        int i,j;
        char a;
        double soma=0.0;
        cout<<fixed<<setprecision(1);
        cin>>a;
        for(i=0; i<12; i++)
        {
                for(j=0; j<12; j++)
                {
                        cin>>M[i][j];
                        if(j>i && (i+j)>11)
                        {
                                soma+=M[i][j];
                        }
                }
        }
        if(a=='S') cout<<soma<<endl;
        else cout<<soma/30.0<<endl;
}
/* Alhamdulillah */
